// Tests common functions (sin, cos, exp, ln, sqrt)
//output: value of 4 when plugged into the following functions

program test4;
var
val : real = 4.0;

begin
//writeln('If val is 1 then the');
writeln('ln of val is ', ln(val));
writeln('exp of val is ', exp(val));
writeln('sin of val is ', sin(val));
writeln('cos of val is ', cos(val));
writeln('sqrt of val is ', ln(val));

end.